<div class="container">
  <h1>ジャンル編集</h1>
  <div class="row">
    <div class="col-md-4">
      <%= form_with model: genre, local:true do |f| %>
      <h3>
        <%= f.label :ジャンル名 %>
        <%= f.text_field :genre_name %>
        <%= f.submit '変更を保存', class: "btn btn-success" %>
      </h3>
      <% end %>
    </div>
  </div>
</div>